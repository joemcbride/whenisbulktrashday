  "description": "Bulk trash day website",
  "main": "server.js",
  "scripts": {
    "build": "yarn build-lib && NODE_ENV=development node ./bin/project build",
    "build-prod": "yarn build-lib && NODE_ENV=production node ./bin/project build",
    "build-lib": "babel src/modules --out-dir lib",
    "start": "yarn build-prod && NODE_ENV=production node ./.build/server.js",
    "start-dev": "yarn build && node ./.build/server.js",
    "dev": "nodemon --watch src --exec 'yarn start-dev'",
